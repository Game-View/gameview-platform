# Redis Connection (get from Upstash console)
# Use the "Redis URL" NOT the "REST URL"
REDIS_URL=redis://default:xxxxx@xxxxx.upstash.io:6379

# Processing Paths (adjust for your machine)
GVCORE_CLI_PATH=/path/to/gvcore-cli
BRUSH_PATH=/opt/brush
WORK_DIR=/tmp/gv-processing

# Supabase Storage (for uploading outputs)
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_SERVICE_ROLE_KEY=xxxxx

# Worker Settings
WORKER_CONCURRENCY=1
